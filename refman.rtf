00025     currTcpSocket->write({\cf22 "Hello, World!!! I am echo server!\\r\\n"});\par
00026     connect(currTcpSocket, &QTcpSocket::readyRead, {\cf17 this},\par
00027             &MyTcpServer::slotServerRead);\par
00028     connect(currTcpSocket, &QTcpSocket::disconnected, {\cf17 this},\par
00029             &MyTcpServer::slotClientDisconnected);\par
00030     tcpSockets.push_back(currTcpSocket);\par
00031   \}\par
00032 \}\par
