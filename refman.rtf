00034 {\cf18 void} MyTcpServer::slotServerRead() \{\par
00035   QByteArray array;\par
00036   QTcpSocket *currTcpSocket = (QTcpSocket *)sender();\par
00037   {\cf19 while} (currTcpSocket->bytesAvailable() > 0) \{\par
00038     array.append(currTcpSocket->readAll());\par
00039   \}\par
00040   {\cf19 if} (array.right(1) == {\cf22 "\\n"}) \{\par
00041     currTcpSocket->write(ServerFunctions::parse(array));\par
00042   \}\par
00043 \}\par
00044 \par
00045 {\cf18 void} MyTcpServer::slotClientDisconnected() \{\par
00046   QTcpSocket *currTcpSocket = (QTcpSocket *)sender();\par
00047   currTcpSocket->close();\par
00048 \}\par
}
